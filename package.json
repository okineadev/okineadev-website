		"@vue/compiler-sfc": "^3.5.13",
		"autoprefixer": "^10.4.20",
		"bun-types": "^1.2.3",
		"prettier": "^3.4.2",
		"sass-embedded": "^1.83.0",
		"typescript": "^5.7.2",
		"unplugin-icons": "^22.1.0",
		"unplugin-vue-components": "^28.4.0",
