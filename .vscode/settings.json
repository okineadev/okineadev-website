{
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"[biome_syntax_tree]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[javascript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[typescript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[vue]": {
		"editor.defaultFormatter": "Vue.volar"
	},
	"[markdown]": {
		"editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
	},
	"vite.devCommand": "bun run dev",
	"editor.formatOnSave": true,
	"github.copilot.chat.codeGeneration.instructions": [
		{
			"file": "/architecture.md"
		}
	],
	"github.copilot.chat.codeGeneration.useInstructionFiles": true,
	"links.resources": [
		{
			"url": "https://github.com/okineadev/okineadev-website",
			"title": "GitHub Repo"
		},
		{
			"url": "https://okinea.dev",
			"title": "This Website"
		},
		{
			"url": "https://github.com/okineadev/okineadev-website/issues",
			"title": "Issues",
			"description": "Report bugs, suggest features, or ask questions."
		},
		{
			"url": "https://github.com/okineadev/okineadev-website/blob/main/NOTES.md",
			"title": "Notes and attributions",
			"description": "Notes and attributions for this website."
		}
	],
	"material-icon-theme.activeIconPack": "vue"
}
